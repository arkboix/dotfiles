{
    "layer": "top",
"position": "bottom",
    "height": 30,
    "spacing": 4,
    "modules-left": ["hyprland/workspaces", "hyprland/submap", "custom/media"],
    "modules-center": ["hyprland/window"],
    "modules-right": ["custom/arkscript", "pulseaudio", "network", "cpu", "memory", "temperature", "backlight", "battery", "clock", "tray"],

    "hyprland/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "format": " {name} {icon}",
        "format-icons": {
            "1": "ÔÇ¨",
            "2": "ÔÅÑ",
            "3": "ÔÑ†",
            "4": "Ôâ∫",
            "5": "ÔáÄ",
            "urgent": "",
            "focused": "",
            "default": ""
        }
    },

    "hyprland/submap": {
        "format": "<span style=\"italic\">{}</span>"
    },

    "tray": {
        "spacing": 10
    },

    "clock": {
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "format": " {:%H:%M}",
        "format-alt": " {:%Y-%m-%d}"
    },

    "cpu": {
        "format": " Ôãõ {usage}%",
        "tooltip": true
    },

    "memory": {
        "format": " Ôî∏ {}%"
    },

    "temperature": {
        "critical-threshold": 80,
        "format": "Ôù´ {temperatureC}¬∞C",
        "format-icons": ["Ôù´", "", ""]
    },

    "backlight": {
        "format": " ÔÜÖ {percent}%",
        "format-icons": ["", "", "", "", "", "", "", "", ""]
    },

    "battery": {
        "states": {
            "good": 95,
            "warning": 30,
            "critical": 15
        },
        "format": "{icon} {capacity}%",
        "format-charging": " {capacity}%",
        "format-plugged": " {capacity}%",
        "format-alt": "{icon} {time}",
        "format-icons": ["ÔâÑ", "ÔâÉ", "ÔâÇ", "ÔâÅ", "ÔâÄ"]
    },

    "network": {
        "format-wifi": " Ôá´ {essid} ({signalStrength}%)",
        "format-ethernet": " {ipaddr}/{cidr}",
        "tooltip-format": " {ifname} via {gwaddr}",
        "format-linked": " {ifname} (No IP)",
        "format-disconnected": "‚ö† Disconnected",
        "format-alt": "{ifname}: {ipaddr}/{cidr}"
    },

    "pulseaudio": {
        "format": "{icon} {volume}% {format_source}",
        "format-bluetooth": "{icon} {volume}% {format_source}",
        "format-bluetooth-muted": " {icon} {format_source}",
        "format-muted": " {format_source}",
        "format-source": " {volume}%",
        "format-source-muted": "",
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "",
            "headset": "Ôñê",
            "phone": "Ôèç",
            "portable": "Ôèâ",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
        },
        "on-click": "pavucontrol"
    },

    "custom/media": {
        "format": "{icon} {}",
        "return-type": "json",
        "max-length": 40,
        "format-icons": {
            "spotify": "",
            "default": "üéú"
        },
        "escape": true,
        "exec": "$HOME/.config/waybar/mediaplayer.py 2> /dev/null"
    },

    "custom/arkscript": {
        "format": " {}",
        "return-type": "text",
        "interval": 10,
        "exec": "echo SCRIPTS",
        "tooltip": true,
        "on-click": "~/arkscripts/main.sh click"
    }
}
