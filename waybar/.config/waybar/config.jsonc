    "modules-right": [ "custom/cheatsheet", "custom/cpuh", "custom/animation", "pulseaudio",  "memory", "battery","tray","clock", "custom/power", "custom/vm", "custom/calendar","group/sysext"],

    "hyprland/workspaces": {
        "disable-scroll": false,
